  "webServices": {
    "first": {
      "mockFile": "king.json",
      "contentType": "foobar",
      "verbs": ["get"]
    },
    "second": {
      "verbs": ["delete"],
      "httpStatus": 204
    },
    "protected": {
      "mockFile": "king.json",
      "httpStatus": 403,
      "verbs": ["put"]
